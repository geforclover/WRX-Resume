<!--index.wxml-->
<view class='container __secColor'>
  <view class='container-board __mainColor {{animatHeightClass}}' style='height: {{animatHeight}}'>
    <text class='container-title'>Debug</text>
    <text class='container-title'>&Write Bug</text>
    <text class='container-float-title'>W&D</text>
  </view>
  <view wx:if='{{showRing}}' bind:touchmove='touchMoveFunc' bind:touchend='touchMoveEnd' style='background'>
    <view class='container-line __mainColor'></view>
    <view class='container-circle __mainColor'></view>
  </view>
  <view class='content'>
    <!-- <text wx:if='{{showText}}' class='content-mainText'>awodnawidonawoidnaiondawiodaw</text> -->
    <view class='content-icon'><icon class="iconfont xiahua" style='font-size: 50px; color: rgba(9,25,79,0.6);'></icon></view>
  </view>
</view>